{
  "name": "dfc-app-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/sora": "^4.5.0",
    "@metamask/detect-provider": "^1.2.0",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@typescript-eslint/parser": "^4.33.0",
    "@usedapp/core": "^0.10.0",
    "@walletconnect/web3-provider": "^1.6.6",
    "axios": "^0.25.0",
    "flag-icons": "^6.1.1",
    "framer-motion": "^4.1.17",
    "magic-sdk": "7.0.0",
    "moralis": "^1.2.4",
    "patch-package": "^6.4.7",
    "react": "^17.0.2",
    "react-blockies": "^1.4.1",
    "react-dom": "^17.0.2",
    "react-loading-screen": "^0.0.17",
    "react-moralis": "^1.2.1",
    "react-query": "^3.34.12",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "clean": "npm run lint-fix && npm run pretty",
    "start": "react-scripts start",
    "dev": "react-scripts dev",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx src",
    "lint-fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts,.tsx ./src",
    "pretty": "prettier --write src/",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browser": {
    "os": false,
    "https": false,
    "http": false,
    "stream": false,
    "util": "./node_modules/util",
    "url": "./node_modules/url",
    "assert": false,
    "crypto": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.5.1",
    "react-error-overlay": "6.0.9",
    "redux-devtools-extension": "^2.13.9"
  }
}
